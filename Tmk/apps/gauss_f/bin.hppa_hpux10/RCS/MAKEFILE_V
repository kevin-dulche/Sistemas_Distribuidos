10.4
log
@Specify optimization level to the Fortran compiiler.  Use the "Extended
ANSI C" flag to the C compiler.
@
text
@ #
 # $Id: Makefile,v 10.3 1997/11/09 00:18:54 alc Exp alc $
 #
ARCH	= hppa_hpux10

CC	= cc
CFLAGS	= -Ae -O

FC	= fort77
FFLAGS	= -O2 +ppu

MACHDEP_OBJS= common_symbol.o
MACHDEP_LIBS= +U77

all: Gauss.udp

include ../.common

common_symbol.o: $(TmkLIB)/support/common_symbol.s
	$(AS) $(ASFLAGS) $(TmkLIB)/support/common_symbol.s
@


