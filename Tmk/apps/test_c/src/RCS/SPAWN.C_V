#include <sys/time.h>
#include <stdio.h>

#include "Tmk.h"

main(argc, argv)
	int		argc;
	char	       *argv[];
{
	int		c, i;

	while ((c = getopt(argc, argv, "")) != -1)
		switch (c) {
		}

	Tmk_startup(argc, argv);

	if (Tmk_proc_id == 0)
		Tmk_spawn("acpc.cs.rice.edu", argc, argv);

	Tmk_barrier(0);

	Tmk_exit(0);
}
