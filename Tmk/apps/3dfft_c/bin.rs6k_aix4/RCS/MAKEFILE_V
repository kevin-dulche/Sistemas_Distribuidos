d15 2
a16 2
	@@if [ -f $(TmkLIB)/libTmk_udp.a    ]; then $(MAKE) fft.udp    ; fi
	@@if [ -f $(TmkLIB)/libTmk_udp_ll.a ]; then $(MAKE) fft.udp_ll ; fi
